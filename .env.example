# Discord Bot Token
DISCORD_TOKEN=your_bot_token_here

# Role name or ID to target for disconnection
TARGET_ROLE_NAME=TimeTracked

# Disconnect time in UTC (24-hour format, e.g., 03:00 for 3AM)
DISCONNECT_TIME=03:00

# Channel ID for notifications (optional, leave empty to disable)
NOTIFICATION_CHANNEL_ID=

# Voice channel IDs to disconnect from (comma-separated, leave empty for all voice channels)
TARGET_VOICE_CHANNELS=
